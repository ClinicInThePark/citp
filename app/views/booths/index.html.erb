
<% provide(:title,"All Organizations") %>
<h3>All Booths</h3>

<hr>
	<div class="booths">
		<table class="table table-striped">
			<thead>
				<tr>
					<th><b>Organization</b></th>
				</tr>
			</thead>
			<%@booths.each do |b|%>
				 <tr>	
				    <td><%=link_to b.name, b%></td>
				 <tr>
			<%end%>
		</table>
	</div>
	<br>
	<div class="creation"><%=link_to "Add New Organization", new_booth_path, class: "btn btn-large btn-primary"%></div>
</hr>
