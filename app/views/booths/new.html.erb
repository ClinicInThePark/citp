<% provide(:title ,"Add organization") %>

<div class="new-edit-form">
<div class="form">
	<div class="form-group">
		<h3 class="new-edit">New Organization</h3>
		<%= form_for(@booth) do |f| %>
			<%= render 'shared/error_messages', object: f.object %> 

			<%=f.label :Name %>
			<%=f.text_field :name, class:"form-control" %>
			<br>
			<%=f.label :Description %>
			<%=f.text_area :description, class:"form-control" %>
			<br>
			<%=f.label :Service %>
			<%=f.text_area :service, class:"form-control" %>
			<br>
			<%=f.submit "Add Organization", class: "btn btn-large btn-primary" %>

		<% end %>
	</div>
</div>

